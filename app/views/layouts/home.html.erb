      </div>
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <%= bootstrap_flash %>
            <%= yield %>
          </div>
        </div><!--/row-->



      </div> <!-- /container -->
      <footer class="footer">
    <nav class="navbar navbar-default navbar-custom">
        <div class="container-fluid  text-center">
              <ul class="nav navbar-nav">
            <li><%= link_to "Link1", "/path1"  %></li>
            <li><%= link_to "Link2", "/path2"  %></li>
            <li><%= link_to "Link3", "/path3"  %></li>
